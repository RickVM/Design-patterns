        <Label x:Name="labelVisibility" Content="visibility" HorizontalAlignment="Left" Margin="10,151,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.557,2.313"/>
        <Label x:Name="labelTemperature" Content="Temperature:" HorizontalAlignment="Left" Margin="10,57,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.557,2.313"/>
